{
  "name": "i18next-react-router",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "react-router",
    "i18n",
    "i18next",
    "ssr",
    "react-router-i18next",
    "remix-i18next"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build"
  },
  "peerDependencies": {
    "i18next": "*",
    "react-router": ">=7",
    "react-i18next": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*"
  }
}
